      .btn-custom {
        background-color: #007bff;
        color: #fff;
        border-color: #007bff;
      }

      .btn-custom:hover {
        background-color: #0069d9;
        border-color: #0062cc;
      }
      .nav-link:hover {
        background-color: rgba(255, 255, 255, 0.1);
      }

      .nav-link {
        width: 200px;
      }

      .navbar {
        background-color: #9683ec;
        display: flex;
        flex-direction: column;
        justify-content: flex-start;
      }
    </style>
